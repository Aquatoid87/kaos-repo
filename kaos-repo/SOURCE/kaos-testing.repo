[kaos-testing]
name=ESSENTIAL KAOS Testing (Unstable) Repo for RHEL$releasever/CentOS$releasever - $basearch
baseurl=https://yum.kaos.st/$releasever/testing/$basearch
failovermethod=priority
priority=1
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ESSENTIALKAOS

[kaos-testing-src]
name=ESSENTIAL KAOS Testing (Unstable) Repo for RHEL$releasever/CentOS$releasever - $basearch
baseurl=https://yum.kaos.st/$releasever/testing/SRPMS
failovermethod=priority
priority=1
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ESSENTIALKAOS
