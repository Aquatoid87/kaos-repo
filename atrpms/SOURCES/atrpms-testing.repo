[atrpms-testing]
name=Red Hat Enterprise Linux $releasever - $basearch - ATrpms testing
failovermethod=priority
baseurl=http://mirror.yandex.ru/fedora/atrpms/el$releasever-$basearch/atrpms/testing
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-atrpms
exclude=setuptools

[atrpms-testing-source]
name=Red Hat Enterprise Linux $releasever - $basearch - ATrpms testing - Source
failovermethod=priority
baseurl=http://mirror.yandex.ru/fedora/atrpms/src/el$releasever-$basearch/atrpms/testing
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-atrpms
exclude=setuptools
