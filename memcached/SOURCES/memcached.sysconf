# Sysconfig for memcached service

# Port
PORT=11211

# User
USER="memcached"

# Max connection number
MAX_CONN=1024

# Cache size in Mb
CACHE_SIZE=64

# Logging 0-3 (0 - disabled 3 - most verbose)
LOG_LEVEL=0

# Path to log file
LOG_FILE="/var/log/memcached/memcached.log"

# Custom options
OPTIONS=""
